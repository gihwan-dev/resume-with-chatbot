GOOGLE_GENERATIVE_AI_API_KEY=
FIREBASE_PRIVATE_KEY=
FIREBASE_CLIENT_EMAIL=
PUBLIC_FIREBASE_PROJECT_ID=

# Notion API
NOTION_API_TOKEN=

# ClickUp API
# Get API token from: ClickUp Settings > Apps > API Token
CLICKUP_API_TOKEN=

# IMPORTANT: TEAM_ID and WORKSPACE_ID should both be the numeric Workspace ID
# In ClickUp API v2, "team_id" actually refers to the Workspace
# Find it at: ClickUp Settings > Workspaces > Look at the URL for the workspace ID
# Example: "25540965" (numeric format, NOT UUID)
CLICKUP_TEAM_ID=
CLICKUP_WORKSPACE_ID=

# Your ClickUp user ID (numeric)
# Find it via API: GET https://api.clickup.com/api/v2/user
CLICKUP_USER_ID=