{
  "name": "web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "node scripts/build-vault.mjs && astro build",
    "build:vault": "node scripts/build-vault.mjs",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "a11y:axe": "playwright test e2e/accessibility.spec.ts --project=chromium",
    "a11y:lighthouse": "mkdir -p ./lighthouse && lighthouse http://localhost:4321 --only-categories=accessibility --output=html --output=json --output-path=./lighthouse/accessibility --quiet --chrome-flags='--headless=new --no-sandbox'"
  },
