{
  "name": "ai-history-collector",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI tool for collecting Claude Code history",
  "main": "dist/index.js",
  "bin": {
    "collect-history": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "collect": "node dist/index.js",
    "dev": "tsc --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "commander": "^13.1.0",
    "date-fns": "^4.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/node": "^22.15.21",
    "typescript": "^5.9.3"
  }
}
